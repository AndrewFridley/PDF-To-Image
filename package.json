        "npm": "^5.2.0",
        "react": "^15.6.1",
        "react-dom": "^15.6.1",
        "react-dropzone": "^4.1.1",
        "react-hot-loader": "^3.0.0-beta.7",
        "react-popup": "^0.8.0"
